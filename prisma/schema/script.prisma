model Script {
  id        Int      @id @default(autoincrement())
  name      String
  script    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  interactScriptId Int?
  interactScript   interactScript? @relation(fields: [interactScriptId], references: [id], onDelete: SetNull)
  ScriptData       ScriptData[]
}
